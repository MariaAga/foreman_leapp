<%#
kind: job_template
name: Run preupgrade via leapp
job_category: Commands
description_format: 'Upgradeability check for rhel7 host'
provider_type: SSH
template_inputs:
- name: debug
  description: enabling --debug option of leapp
  input_type: user
  required: false
  options: "true\nfalse"
%>

<% if input("debug") == "True" %>
leapp preupgrade --debug
<% else %>
leapp preupgrade
<% end%>
